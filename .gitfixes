.bash_aliases:9: alias gitaligntoremote='git reset --hard @{u}'
.bash_aliases:10: alias notetake='vim $(date +%d-%m-%Y_%H%M%S)'
.bash_aliases:11: cleandockercontainers(){
.bash_aliases:12:+	hash docker 2>/dev/null || { echo >&2 "I require docker but it's not installed.  Aborting."; return; }	
.bash_aliases:13: 	read -p "This will wipe all containers off this machine!! Enter to continue..."
.bash_aliases:14: 	docker rm $(docker ps -a -q)
.bash_aliases:15: }
.bash_aliases:16: cleandockerimages(){
.bash_aliases:17:+	hash docker 2>/dev/null || { echo >&2 "I require docker but it's not installed.  Aborting."; return; }
.bash_aliases:18: 	read -p "This will wipe all images off this machine!! Enter to continue..."
.bash_aliases:19: 	docker rmi $(docker images -a -q)
.bash_aliases:20: }
